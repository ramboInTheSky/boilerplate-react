{
  "name": "boilerplate-react",
  "version": "0.1.8",
  "author": "Amido",
  "description": "A React / Redux boilerplate with Jest and Enzyme for testing, AirB&B linting and EsDocs",
  "license": "GNU GPLv3",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/amido/boilerplate-react.git"
  },
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-core": "6.17.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "17.0.2",
    "babel-loader": "6.2.7",
    "babel-preset-react-app": "^2.0.1",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.0",
    "detect-port": "1.0.1",
    "dotenv": "2.0.0",
    "enzyme": "^2.7.1",
    "esdoc": "^0.5.2",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-config-react-app": "^0.5.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.2",
    "jest": "^19.0.2",
    "json-loader": "0.5.4",
    "node-sass": "^4.3.0",
    "object-assign": "4.1.0",
    "path-exists": "2.1.0",
    "postcss-loader": "1.0.0",
    "promise": "7.1.1",
    "react-addons-test-utils": "^15.4.2",
    "react-dev-utils": "^0.4.2",
    "recursive-readdir": "2.1.0",
    "sass-loader": "^4.1.1",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "lodash.concat": "^4.5.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.2.0",
    "yargs": "^7.1.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "docs": "esdoc -c config/esdoc.json",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint --fix -c .eslintrc.yml --ignore-path .eslintignore ./src",
    "coverage": "node scripts/test.js --env=jsdom --coverage",
    "e2e": "node scripts/e2e-run.js",
    "e2e:stop": "docker-compose -f config/docker-compose.yml stop",
    "e2e:install": "node scripts/e2e-install.js",
    "e2e:uninstall": "node scripts/e2e-install.js --uninstall",
    "e2e:update": "docker-compose -f config/docker-compose.yml rm -f e2e && docker-compose -f config/docker-compose.yml pull e2e"
  }
}
